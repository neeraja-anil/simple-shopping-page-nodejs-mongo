<section>
  <div class="container ">
    <div class="row float-end m-4">
    </div>
    <table class="table">
      <thead>
        <tr>
          <th scope="col">#</th>
          <th scope="col">name</th>
          <th scope="col">category</th>
          <th scope="col">description</th>
          <th scope="col">price</th>
          <th scope="col">image</th>
        </tr>
      </thead>
      <tbody>
        {{#each products}}
        <tr>
          <th scope="row">1</th>
          <td>{{this.name}}</td>
          <td>{{this.category}}</td>
          <td>{{this.description}}</td>
          <td>{{this.price}}</td>
          <td><img style="width: 50px; height:50px" src="{{this.image}}"></td>
        </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</section>